import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    double suma = 0;
    
    Scanner stdin = new Scanner(System.in);

    while(stdin.hasNextDouble()) {
        suma += stdin.nextDouble();
    }
    stdin.close();

    if(suma % 1 == 0){
      System.out.printf("%n%.0f", suma);
    } else {
      System.out.println(suma);
    }   
  }
}
